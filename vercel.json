{
  "name": "comment",
  "github": {
    "silent": true
  },
  "rootDirectory": "example",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "builds": [
    {
      "src": "example/index.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "example/index.cjs"
    }
  ]
}
